@article{cyr_technical_2015,
	title = {Technical note: a multi-dimensional description of knee laxity using radial basis functions},
	volume = {18},
	shorttitle = {Technical note},
	doi = {10.1080/10255842.2014.946913},
	abstract = {The net laxity of the knee is a product of individual ligament structures that provide constraint for multiple degrees of freedom (DOF). Clinical laxity assessments are commonly performed along a single axis of motion, and lack analyses of primary and coupled motions in terms of translations and rotations of the knee. Radial basis functions (RBFs) allow multiple DOF to be incorporated into a single method that accounts for all DOF equally. To evaluate this method, tibiofemoral kinematics were experimentally collected from a single cadaveric specimen during a manual laxity assessment. A radial basis function (RBF) analysis was used to approximate new points over a uniform grid space. The normalized root mean square errors of the approximated points were below 4\% for all DOF. This method provides a unique approach to describing joint laxity that incorporates multiple DOF in a single model.},
	number = {15},
	journal = {Computer Methods in Biomechanics and Biomedical Engineering},
	author = {Cyr, Adam J. and Maletsky, Lorin P.},
	month = nov,
	year = {2015},
	keywords = {Envelope of Motion, Knee Laxity, Passive Constraint, Radial Basis Function},
	pages = {1674--1679},
	file = {IngentaConnect Full Text PDF:/home/will/.zotero/zotero/gs4a8wf2.default/zotero/storage/QW7EBRSR/Cyr and Maletsky - 2015 - Technical note a multi-dimensional description of.pdf:application/pdf}
}

@article{grood_joint_1983,
	title = {A {Joint} {Coordinate} {System} for the {Clinical} {Description} of {Three}-{Dimensional} {Motions}: {Application} to the {Knee}},
	volume = {105},
	issn = {0148-0731},
	shorttitle = {A {Joint} {Coordinate} {System} for the {Clinical} {Description} of {Three}-{Dimensional} {Motions}},
	url = {http://dx.doi.org/10.1115/1.3138397},
	doi = {10.1115/1.3138397},
	abstract = {The experimental study of joint kinematics in three dimensions requires the description and measurement of six motion components. An important aspect of any method of description is the ease with which it is communicated to those who use the data. This paper presents a joint coordinate system that provides a simple geometric description of the three-dimensional rotational and translational motion between two rigid bodies. The coordinate system is applied to the knee and related to the commonly used clinical terms for knee joint motion. A convenient characteristic of the coordinate system shared by spatial linkages is that large joint displacements are independent of the order in which the component translations and rotations occur.},
	number = {2},
	urldate = {2016-03-07},
