	journal = {The American Journal of Sports Medicine},
	author = {Steensen, Robert N. and Dopirak, Ryan M. and Mcdonald, William G.},
	month = sep,
	year = {2004},
	keywords = {anatomy, knee, dislocation, isometry, medial patellofemoral ligament (MPFL), patella, patellar dislocation, recurrent dislocation, soft-tissue restraints},
