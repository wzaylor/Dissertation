	month = mar,
	year = {2016},
	keywords = {ACL, Displacement, In-situ, Knee flexion, Slackness, Tension, Recruitment},
	pages = {209--215},
	file = {ScienceDirect Full Text PDF:/home/will/.zotero/zotero/gs4a8wf2.default/zotero/storage/BV2357ZE/Rachmat et al. - 2016 - In-situ mechanical behavior and slackness of the a.pdf:application/pdf;ScienceDirect Snapshot:/home/will/.zotero/zotero/gs4a8wf2.default/zotero/storage/HN5ZIZV9/S1350453315002696.html:text/html}
