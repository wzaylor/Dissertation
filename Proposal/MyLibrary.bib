@article{petersen_anatomy_2007,
	title = {Anatomy of the {Anterior} {Cruciate} {Ligament} with {Regard} to {Its} {Two} {Bundles}},
	volume = {454},
	issn = {0009-921X},
	url = {https://journals.lww.com/clinorthop/Fulltext/2007/01000/Anatomy_of_the_Anterior_Cruciate_Ligament_with.9.aspx},
	doi = {10.1097/BLO.0b013e31802b4a59},
	abstract = {The anterior cruciate ligament (ACL) consists of two major fiber bundles, namely the anteromedial and posterolateral bundle. When the knee is extended, the posterolateral bundle (PL) is tight and the anteromedial (AM) bundle is moderately lax. As the knee is flexed, the femoral attachment of the ACL becomes a more horizontal orientation; causing the AM bundle to tighten and the PL bundle to relax. There is some degree of variability for the femoral origin of the anterome-dial and posterolateral bundle. The anteromedial bundle is located proximal and anterior in the femoral ACL origin (high and deep in the notch when the knee is flexed at 90°); the posterolateral bundle starts in the distal and posterior aspect of the femoral ACL origin (shallow and low when the knee is flexed at 90°). In the frontal plane the anteromedial bundle origin is in the 10:30 clock position and the postero-lateral bundle origin in the 9:30 clock position. At the tibial insertion the ACL fans out to form the foot region. The anteromedial bundle insertion is in the anterior part of the tibial ACL footprint, the posterolateral bundle in the posterior part. While the anteromedial bundle is the primary restraint against anterior tibial translation, the posterolateral bundle tends to stabilize the knee near full extension, particularly against rotatory loads.},
	language = {en-US},
	urldate = {2018-05-11},
	journal = {Clinical Orthopaedics and Related Research®},
	author = {Petersen, Wolf and Zantop, Thore},
	month = jan,
	year = {2007},
	pages = {35},
	file = {Petersen and Zantop - 2007 - Anatomy of the Anterior Cruciate Ligament with Reg.pdf:/home/will/.zotero/zotero/gs4a8wf2.default/zotero/storage/M77ND7TF/Petersen and Zantop - 2007 - Anatomy of the Anterior Cruciate Ligament with Reg.pdf:application/pdf;Snapshot:/home/will/.zotero/zotero/gs4a8wf2.default/zotero/storage/HD4IYYUB/Anatomy_of_the_Anterior_Cruciate_Ligament_with.9.html:text/html}
}

