	file = {ScienceDirect Full Text PDF:/home/will/.zotero/zotero/gs4a8wf2.default/zotero/storage/I2G2U4X7/Hull et al. - 1996 - Strain in the medial collateral ligament of the hu.pdf:application/pdf;ScienceDirect Snapshot:/home/will/.zotero/zotero/gs4a8wf2.default/zotero/storage/38LTWQU3/0021929095000461.html:text/html}
}

@article{weiss_three-dimensional_2005-1,
	series = {Advances in the finite element modelling of soft tissue deformation},
